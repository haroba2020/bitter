<%- include('partials/header'); -%>

<div class="card-wrapper">
    <% let count = 0; %>
    <% posts.forEach(post => { %>
        <% count++ %>
        <% if(count<=5){ %>
            <div class="card">
                <p>by <%= post.owner %></p>
                <a href="/posts/<%= post._id %>"> <h1><%= post.title %></h1> </a>
                <h2><%= post.content %></h2>
            </div>
        <% } %>
    <% }); %>
</div>        
<%- include('partials/footer'); -%>